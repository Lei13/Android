<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <TextView 
        android:id="@+id/http_show_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <Button 
        android:id="@+id/http_request_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="http -Get请求连接"/>
    
    <Button 
        android:id="@+id/http_client_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="http Client请求连接"/>
    <Button 
        android:id="@+id/http_client_post_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="http Client-post传参请求连接"/>
    <Button 
        android:id="@+id/http_util_post_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="http util-post传参请求连接"/>
     <Button 
        android:id="@+id/http_util_test_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="http 工具类连接测试"/>
    

</LinearLayout>
